<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:visibility="gone"
    android:id="@+id/carrenzheng_zbg_info">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/txt_39dp"
        android:orientation="horizontal"
        android:background="@color/white">
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:text="自备柜车牌号:"
            android:paddingLeft="@dimen/txt_15dp"
            android:textColor="@color/black"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
        <EditText
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:hint="请输入自备柜车牌号"
            android:background="@null"
            android:id="@+id/carrc_common_zbgcode"
            android:textColor="#666666"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/txt_39dp"
        android:orientation="horizontal"
        android:background="@color/white">
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:text="海关编码:"
            android:paddingLeft="@dimen/txt_15dp"
            android:textColor="@color/black"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
        <EditText
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:hint="请输入海关编码"
            android:background="@null"
            android:id="@+id/carrc_common_hgcode"
            android:textColor="#666666"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/txt_39dp"
        android:orientation="horizontal"
        android:background="@color/white">
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:text="柜号:"
            android:paddingLeft="@dimen/txt_15dp"
            android:textColor="@color/black"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
        <EditText
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:hint="请输入自备柜号"
            android:background="@null"
            android:id="@+id/carrc_common_zbgnum"
            android:textColor="#666666"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/txt_39dp"
        android:orientation="horizontal"
        android:background="@color/white">
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:text="最大载重(吨):"
            android:paddingLeft="@dimen/txt_15dp"
            android:textColor="@color/black"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
        <EditText
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:hint="请输入最大载重"
            android:background="@null"
            android:id="@+id/carrc_common_zaizhong"
            android:textColor="#666666"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/txt_39dp"
        android:orientation="horizontal"
        android:background="@color/white"
        >
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:text="头重"
            android:paddingLeft="@dimen/txt_15dp"
            android:textColor="@color/black"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
        <EditText
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:hint="自备柜头重"
            android:background="@null"
            android:singleLine="true"
            android:id="@+id/carrenzheng_common_headweight"
            android:textColor="#666666"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:text="柜重"
            android:paddingLeft="@dimen/txt_15dp"
            android:textColor="@color/black"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
        <EditText
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:hint="自备柜柜重"
            android:singleLine="true"
            android:background="@null"
            android:id="@+id/carrenzheng_common_guiweight"
            android:textColor="#666666"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:text="架重"
            android:paddingLeft="@dimen/txt_15dp"
            android:textColor="@color/black"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
        <EditText
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:hint="自备柜架重"
            android:singleLine="true"
            android:background="@null"
            android:id="@+id/carrenzheng_common_jiaweight"
            android:textColor="#666666"
            android:textSize="@dimen/txt_14"
            android:gravity="center_vertical"
            />
    </LinearLayout>
</LinearLayout>